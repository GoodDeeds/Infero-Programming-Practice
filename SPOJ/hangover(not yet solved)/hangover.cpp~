#include<iostream>
using namespace std;
int main()
{
	long double n;
	int temp;
	cin>>n;
	temp=100*n;
	n=(long double)(temp)/100.0;
	
	while(n!=0)
	{
		
		int i=2;
		long double sum=0,sum2=0;
		while(sum2<n)
		{
			sum+=%0.10(long double)(1/(long double)i)f;
			sum2=sum;
			temp=100*sum2;
			sum2=(long double)(temp)/100.0;
			i++;
			
		}
		
		cout<<i-2<<endl;
		cin>>n;
	}
	return 0;
}
