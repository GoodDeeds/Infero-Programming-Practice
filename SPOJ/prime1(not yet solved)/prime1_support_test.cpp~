#include<iostream>
#include<cmath>
using namespace std;
struct node
{
	long long int data;
	struct node *next;
}*start=NULL,*p,*tail=NULL;
typedef struct node nod;
int main()
{
	long long int n,i,j,size=0;
	int arr[1000],flag;
	cin>>n;
	for(i=2;i<n;i++)
	{
		flag=0;
		for(j=2;j<i;j++)
		{
			if(i%j==0)
			{
				flag=1;
				break;
			}
		}
		if(flag==0)
		{
			p=new nod;
			p->data=i;
			p->next=NULL;
			if(start==NULL)
			{
				start=tail=p;
			}
			else
			{
				tail->next=p;
				tail=p;
			}		
		}
	}
	return 0;	
}
